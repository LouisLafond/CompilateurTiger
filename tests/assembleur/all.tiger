let
    var a := 0
    
    function carre(n: int) =(
        n * n
    )

in
    while a<5 do (
        a := a + 1
    );

    for i := 0 to 3 do (
        for j := 0 to 3 do (
            a := i + j
        )
    );

    if a < 3 then (
        a := 8
    )else(
        a := 7
    );
    
    a := carre(a)
end